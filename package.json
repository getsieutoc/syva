{
  "name": "syva",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "prisma": "prisma",
    "ui:add": "pnpm dlx shadcn-ui@latest add",
    "ui:diff": "pnpm dlx shadcn-ui@latest diff",
    "postinstall": "prisma generate",
    "clean": "rimraf node_modules .next dist && pnpm i",
    "dev": "docker-compose up -d && pnpm run watch && docker-compose down",
    "watch": "trap 'exit 0' SIGINT; concurrently \"next dev\" \"storybook dev -p 6006\"",
    "lint": "eslint ./ --ext .ts,.tsx",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "build-storybook": "storybook build"
  },
  "engines": {
    "node": "20.x"
  },
  "dependencies": {
    "@auth/prisma-adapter": "1.0.15",
    "@next-auth/prisma-adapter": "1.0.7",
    "@prisma/client": "5.8.1",
    "@radix-ui/react-accordion": "1.1.2",
    "@radix-ui/react-alert-dialog": "1.0.5",
    "@radix-ui/react-aspect-ratio": "1.0.3",
    "@radix-ui/react-avatar": "1.0.4",
    "@radix-ui/react-popover": "1.0.7",
    "@radix-ui/react-slot": "1.0.2",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.0",
    "date-fns": "3.3.1",
    "deepmerge": "4.3.1",
    "jotai": "2.6.2",
    "lucide-react": "0.312.0",
    "next": "14.1.0",
    "next-auth": "4.24.5",
    "next-themes": "0.2.1",
    "nodemailer": "6.9.8",
    "react": "18.2.0",
    "react-day-picker": "8.10.0",
    "react-dom": "18.2.0",
    "swr": "2.2.4",
    "tailwind-merge": "2.2.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "7.6.10",
    "@storybook/addon-interactions": "7.6.10",
    "@storybook/addon-links": "7.6.10",
    "@storybook/addon-onboarding": "1.0.10",
    "@storybook/addon-styling-webpack": "^0.0.6",
    "@storybook/blocks": "7.6.10",
    "@storybook/nextjs": "7.6.10",
    "@storybook/react": "7.6.10",
    "@storybook/test": "7.6.10",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.17",
    "concurrently": "8.2.2",
    "eslint": "8.56.0",
    "eslint-config-next": "14.1.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-storybook": "0.6.15",
    "postcss": "8.4.33",
    "prettier": "3.2.4",
    "prettier-plugin-tailwindcss": "0.5.11",
    "prisma": "5.8.1",
    "rimraf": "5.0.5",
    "storybook": "7.6.10",
    "tailwindcss": "3.4.1",
    "tailwindcss-animate": "1.0.7",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "typescript": "5.3.3"
